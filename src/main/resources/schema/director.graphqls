extend type Query {
   directors: [Director]
   director(id: ID!): Director!
}

extend type Mutation {
   newDirector(input: DirectorInput!): Director
}

input DirectorInput {
   name: String!
   age: Int!,
   lastName: String
}

type Director {
   id: ID!
   name: String!
   age: Int!
   lastName: String
   movies: [Movie]
}